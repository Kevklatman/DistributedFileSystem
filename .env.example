# Storage Environment ('local' or 'aws')
STORAGE_ENV=local

# AWS Credentials (only needed if STORAGE_ENV=aws)
AWS_ACCESS_KEY=your_access_key_here
AWS_SECRET_KEY=your_secret_key_here
